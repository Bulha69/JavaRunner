<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<VBox alignment="CENTER" spacing="20.0" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.runner.QuitConfirmController">
    <children>
        <Label text="Are you sure you want to quit?" />
        <Button fx:id="yesButton" mnemonicParsing="false" onAction="#handleYes" prefHeight="50.0" prefWidth="200.0" text="Yes" />
        <Button fx:id="noButton" mnemonicParsing="false" onAction="#handleNo" prefHeight="50.0" prefWidth="200.0" text="No" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>