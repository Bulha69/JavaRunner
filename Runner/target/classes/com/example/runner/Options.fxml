<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<VBox alignment="CENTER" spacing="20.0" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.runner.OptionsController">
    <children>
        <Button fx:id="optionAButton" mnemonicParsing="false" onAction="#handleOptionA" prefHeight="50.0" prefWidth="200.0" text="Option A" />
        <Button fx:id="optionBButton" mnemonicParsing="false" onAction="#handleOptionB" prefHeight="50.0" prefWidth="200.0" text="Option B" />
        <Button fx:id="backButton" mnemonicParsing="false" onAction="#handleBack" prefHeight="50.0" prefWidth="200.0" text="Return to Main Menu" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>